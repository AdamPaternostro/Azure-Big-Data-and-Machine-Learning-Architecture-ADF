				"type": "UTF8"
			},
			{
				"name": "AccountType",
				"type": "UTF8"
			},
			{
				"name": "Operator",
				"type": "UTF8"
			},
			{
				"name": "CustomMembers",
				"type": "UTF8"
			},
			{
				"name": "ValueType",
