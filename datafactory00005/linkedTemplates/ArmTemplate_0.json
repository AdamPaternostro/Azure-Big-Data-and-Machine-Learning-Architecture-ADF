					"outputFileSystem": {
						"type": "string"
					},
					"outputFileDirectory": {
						"type": "string"
					}
				},
				"annotations": [],
				"type": "Binary",
				"typeProperties": {
					"location": {
						"type": "AzureBlobFSLocation",
						"folderPath": {
							"value": "@dataset().outputFileDirectory",
							"type": "Expression"
						},
						"fileSystem": {
							"value": "@dataset().outputFileSystem",
							"type": "Expression"
						}
					}
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/DataLakeStorage')]"
