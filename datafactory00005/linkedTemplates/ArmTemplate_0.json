								"type": "DatasetReference",
								"parameters": {
									"outputFileSystem": {
										"value": "@pipeline().parameters.outputFileSystem",
										"type": "Expression"
									},
									"outputFileDirectory": {
										"value": "@pipeline().parameters.outputFileDirectory",
										"type": "Expression"
									}
								}
							}
						]
					}
				],
				"parameters": {
					"inputFileSystem": {
						"type": "string",
						"defaultValue": "acmeinc"
					},
					"inputFileDirectory": {
						"type": "string",
						"defaultValue": "inbox/adam"
					},
					"outputFileSystem": {
						"type": "string",
						"defaultValue": "raw"
					},
					"outputFileDirectory": {
						"type": "string",
						"defaultValue": "acmeinc/adam"
