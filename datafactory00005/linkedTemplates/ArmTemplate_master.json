{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datafactory00005"
		},
		"DataLakeStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalake00005.dfs.core.windows.net"
		},
		"LandingZoneBlobStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://landingzonestorage00005.dfs.core.windows.net"
		},
		"NYCTaxiData_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://s3.amazonaws.com"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"DataLakeStorage_properties_typeProperties_url": {
						"value": "[parameters('DataLakeStorage_properties_typeProperties_url')]"
					},
					"LandingZoneBlobStorage_properties_typeProperties_url": {
						"value": "[parameters('LandingZoneBlobStorage_properties_typeProperties_url')]"
					},
					"NYCTaxiData_properties_typeProperties_url": {
						"value": "[parameters('NYCTaxiData_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}