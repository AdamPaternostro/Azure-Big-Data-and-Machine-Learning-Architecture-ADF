{
	"name": "CopyLandingDataToDataLake",
	"properties": {
		"activities": [
			{
				"name": "Copy Landing Data",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 3,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "BinarySource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true
						}
					},
					"sink": {
						"type": "BinarySink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings",
							"copyBehavior": "PreserveHierarchy"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "CustomerLandingDataSource",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "CustomerLandingDataDestination",
						"type": "DatasetReference"
					}
				]
			}
		],
		"parameters": {
			"inputFile": {
				"type": "string"
			},
			"inputPath": {
				"type": "string"
			},
			"outputDestination": {
				"type": "string"
			}
		},
		"annotations": []
	}
}