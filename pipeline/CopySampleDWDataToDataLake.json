{
	"name": "CopySampleDWDataToDataLake",
	"properties": {
		"activities": [
			{
				"name": "Copy Sample Data",
				"description": "Copies from the public storage account to your data lake for testing",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "BinarySource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"wildcardFileName": "*.parquet"
						}
					},
					"sink": {
						"type": "BinarySink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings",
							"copyBehavior": "PreserveHierarchy"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "SampleDataSourceBinary",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "SampleDataDestinationBinary",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}